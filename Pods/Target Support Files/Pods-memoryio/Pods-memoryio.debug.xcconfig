GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AVCapture" "${PODS_ROOT}/Headers/Public/NSGIF" "${PODS_ROOT}/Headers/Public/SimpleExif"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AVCapture" "${PODS_CONFIGURATION_BUILD_DIR}/NSGIF" "${PODS_CONFIGURATION_BUILD_DIR}/SimpleExif"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AVCapture" -isystem "${PODS_ROOT}/Headers/Public/NSGIF" -isystem "${PODS_ROOT}/Headers/Public/SimpleExif"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AVCapture" -l"NSGIF" -l"SimpleExif" -framework "AudioToolbox" -framework "CoreMedia"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/..
PODS_ROOT = ${SRCROOT}/../Pods
